{
  "name": "Bpp",
  "displayName": "Bpp",
  "description": "Translator to B++",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "Bpp.translate",
        "category": "B++",
        "title": "Слава Руси"
      },
      {
        "command": "Bpp.ASCII",
        "category": "B++",
        "title": "Показать православную икону"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "clang": "^1.0.9",
    "clangd": "^0.0.0"
  }
}